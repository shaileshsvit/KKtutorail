<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">

<context:annotation-config />

	<bean id="employee" class="com.infotech.model.Employee">
		<constructor-arg name="employeeId" value="43255"></constructor-arg>
		<constructor-arg name="employeeName" value="Sean Murphy"></constructor-arg>
	</bean>

	<bean id="pancard" class="com.infotech.model.Pancard">
		<property name="panHolderName" value="Sean Murphy"></property>
		<property name="panNo" value="HSHS665SS"></property>
	</bean>
</beans>